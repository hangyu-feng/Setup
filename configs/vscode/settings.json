{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.fontFamily": "Cascadia Mono PL",
  "debug.console.fontFamily": "Cascadia Mono PL",
  "debug.console.fontSize": 12,
  "terminal.integrated.fontFamily": "Cascadia Mono PL",
  "terminal.external.windowsExec": "",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "terminal.integrated.fontWeight": "500",
  "editor.fontWeight": "400",
  "editor.tabSize": 2,

  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": true,
  "gitlens.keymap": "none",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.bind.enter.key": false,
  "latex-workshop.view.pdf.invert": 0.85,
  "latex-workshop.view.pdf.invertMode.enabled": "auto",
  "latex-workshop.view.pdf.invertMode.grayscale": 0,

  "[python]": {
    "editor.tabSize": 4
  },
  "python.languageServer": "Pylance",


  "powershell.powerShellDefaultVersion": "pwsh",
  "powershell.powerShellAdditionalExePaths": [
    {
      "versionName": "pwsh",
      "exePath": "C:/Users/VailG/scoop/apps/pwsh/current/pwsh.exe"
    }
  ],
  "editor.renderControlCharacters": false,

  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "dart.debugExternalLibraries": false,
  "dart.debugSdkLibraries": false,
  "remote.SSH.remotePlatform": {
    "UT": "linux"
  },
  "workbench.colorTheme": "One Dark Pro",
  "matlab.matlabpath": "C:/Program Files/MATLAB/R2020b/bin/matlab.exe",
  "settingsSync.ignoredSettings": [
    "-matlab.matlabpath",
    "resmon.show.cputemp"
  ],
  "python.terminal.executeInFileDir": true,
  "resmon.disk.format": "PercentRemaining",
  "resmon.show.cputemp": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "files.associations": {
    "*.pl": "prolog"
  },
  "editor.acceptSuggestionOnEnter": "off",
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.decorations.badges": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.profiles.windows": {
    "pwsh": {
      "path":  "${env:userprofile}/scoop/apps/pwsh/current/pwsh.exe",
      "icon": "terminal-powershell"
    },
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "terminal.integrated.defaultProfile.windows": "pwsh",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.tabSizing": "shrink",
  "editor.fontLigatures": false,
  "window.zoomLevel": 0,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },

  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.enableExperiments": false,
